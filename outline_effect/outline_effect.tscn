func _ready() -> void:
	var tween = create_tween()
	tween.tween_property(self, \"rotation:y\", PI/4.0, 2.0)
	tween.tween_property(self, \"rotation:y\", 0.0, 2.0)
	tween.tween_property(self, \"rotation:y\", -PI/4.0, 2.0)
	tween.tween_property(self, \"rotation:y\", 0.0, 2.0)
	tween.set_loops()
"

